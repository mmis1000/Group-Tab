{

  "manifest_version": 2,
  "name": "Group Tab",
  "version": "0.2.4alpha",

  "description": "Show your pages in group with keyword.",
  
  "applications": {
    "gecko": {
      "id": "firefox.addon.group.tab@mmis1000.me"
    }
  },
  
  "icons": {
    "48": "icons/icon_48.png",
    "96": "icons/icon_96.png"
  },
  
  "browser_action": {
    "default_icon": "icons/page-action.svg",
    "default_title": "Show tabs",
    "default_popup": "popup/menu.html"
  },
  
  "web_accessible_resources": ["tabs/assets/*.json"],
  
  "permissions": [
    "tabs",
    "bookmarks",
    "storage",
    "unlimitedStorage",
    "notifications"
  ],
  
  "background": {
    "page": "tabs/jieba-service.html"
  }
}